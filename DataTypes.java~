public class DataTypes {
  public static void main (String[] args) {
   int number = 3 ;
   double pi = 3.14;
   String name = "Natan";
   boolean ridesBus = true;
   System.out.println(number);
   System.out.println(pi);
   System.out.println(name);
   System.out.println(ridesBus);
   System.out.printf(number+pi+name+ridesBus);
    
  }
  
}  